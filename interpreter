#include "expression.h"
#include "stdbool.h"
#include "parser.h"
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <malloc.h>
#include <ctype.h>
#include "scanner.h"

void GEN_WRITE_VAR_LITERAL(int token, string *attr, int deep){
    switch (token)
	{
		case (INTEGER):
			printf("int@%s", attr->str);
			break;

		case (FLOAT):
			printf("float@%s", attr->str);
			break;

		case (STRING):
			printf("string@%s", attr->str);
			break;
	}
}
